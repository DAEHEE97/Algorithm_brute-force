   "source": [
    "a,b,c,d,e,f = map(int, input().split())\n",
    "\n",
    "for i in range(-999, 999+1):\n",
    "    \n",
    "    for j in range(-999, 999+1):\n",
    "        \n",
    "        if a*i + b*j == c and d*i + e*j == f:\n",
    "            \n",
    "            print(i, j)\n"
   ]
